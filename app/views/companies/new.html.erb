<h1>Update form</h1>

<%= form_for @company do |f| %>
<%= f.label :name %>
<%= f.text_field :name %><br>
<%= f.fields_for :address %>
<%= f.text_field :address %><br>
<%= f.label :country %>
<%= f.text_field :country %><br>
<%= f.label :number_of_floors %>
<%= f.text_field :number_of_floors %><br>
<%= f.label :rent_per_floor %>
<%= f.text_field :rent_per_floor %><br>
<%= f.submit %>
<% end %>

<% @company.errors.messages.each do |message| %>
<p><%= message %></p>
<% end %>
